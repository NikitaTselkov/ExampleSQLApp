<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ExampleSQLApp.Styles">

    <Color x:Key="darkRed">#4B0E19</Color>
    <SolidColorBrush  x:Key="DarkRed" Color="{StaticResource darkRed}" />

    <Color x:Key="darkBlue">#07184A</Color>
    <SolidColorBrush  x:Key="DarkBlue" Color="{StaticResource darkBlue}" />

    <Color x:Key="white">#EAEBED</Color>
    <SolidColorBrush  x:Key="White" Color="{StaticResource white}" />

    <Color x:Key="green">#206A2B</Color>
    <SolidColorBrush  x:Key="Green" Color="{StaticResource green}" />

    <Color x:Key="darkGreen">#06240F</Color>
    <SolidColorBrush  x:Key="DarkGreen" Color="{StaticResource darkGreen}" />

    <Color x:Key="darkYellow">#373105</Color>
    <SolidColorBrush  x:Key="DarkYellow" Color="{StaticResource darkYellow}" />

</ResourceDictionary>